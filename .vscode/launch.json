  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/talk-llama",
      "args": [
        "-mw", "${workspaceFolder}/models/ggml-base.en.bin", 
        "-ml", "${workspaceFolder}/../../models/llama-2-7b-chat/ggml-model-q8_0.gguf", 
        "-p", "Bo", 
        "-t", "8", 
        "-s", "/home/lito/Projects/AI_Stuff/QuestRoomAI/speak.sh",
        "-ngl", "15"
      ],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          },
          {
              "description": "Set Disassembly Flavor to Intel",
              "text": "-gdb-set disassembly-flavor intel",
              "ignoreFailures": true
          }
      ]
    }
  ]
}